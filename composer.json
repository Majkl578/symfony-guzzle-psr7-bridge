{
    "name": "majkl578/symfony-guzzle-psr7-bridge",
    "type": "library",
    "description": "Adapter for Symfony PSR-7 bridge and Guzzle PSR-7 implementation.",
    "keywords": ["http", "psr-7", "http-message", "symfony"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Moravec"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/http-foundation": "^3.0|^4.0",
        "guzzlehttp/psr7": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpstan/phpstan": "^0.7.0",
        "mockery/mockery": "dev-master#225cb715d0",
        "phing/phing": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Majkl578\\SymfonyGuzzlePsr7Bridge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Majkl578\\SymfonyGuzzlePsr7Bridge\\": "tests/"
        }
    }
}
